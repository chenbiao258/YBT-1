  </resultMap>
  <sql id="Base_Column_List">
    trainingtype,qualifiinfo,flag,makedate,maketime,modifydate,modifytime
  </sql>
  
  <select id="selectAllBean" parameterType="com.sinosoft.service.business.db.vo.LaqualitrainVo" resultMap="BaseResultMap">
  	select
		<include refid="Base_Column_List" /> 
		from laqualitrain
		where trainingtype = #{trainingtype} 
  </select>
  
  <select id="selectBytypeAndinfo" parameterType="com.sinosoft.service.business.db.vo.LaqualitrainVo" resultType="java.lang.Integer">
