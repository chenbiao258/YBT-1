<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.service.business.db.dao.tracertMapper">
  <resultMap id="BaseResultMap" type="com.sinosoft.service.business.db.vo.tracertVo">
    <result column="TID" jdbcType="VARCHAR" property="tid" />
    <result column="USERNAME" jdbcType="VARCHAR" property="username" />
    <result column="OPDATE" jdbcType="DATE" property="opdate" />
    <result column="OPTIME" jdbcType="VARCHAR" property="optime" />
    <result column="MATTER" jdbcType="VARCHAR" property="matter" />
    <result column="STATES" jdbcType="VARCHAR" property="states" />
    <result column="JSPNAME" jdbcType="VARCHAR" property="jspname" />
  </resultMap>
    <sql id="Base_Column_List">
 tid,username,opdate,optime,matter,states,jspname
  </sql>
  
  <select id="select" resultMap="BaseResultMap" parameterType="com.sinosoft.service.business.db.vo.tracertVo">
  select * from tracert where 1=1
    <if test="username != null" >
    and username=#{username}
    </if>
  </select>
  <insert id="insert" parameterType="com.sinosoft.service.business.db.vo.tracertVo">
    insert into TRACERT (TID, USERNAME, OPDATE, 
      OPTIME, MATTER, STATES, 
      JSPNAME)
    values (seq_tracert.nextval, #{username,jdbcType=VARCHAR}, #{opdate,jdbcType=DATE}, 
      #{optime,jdbcType=VARCHAR}, #{matter,jdbcType=VARCHAR}, #{states,jdbcType=VARCHAR}, 
      #{jspname,jdbcType=VARCHAR})
  </insert>


 
</mapper>