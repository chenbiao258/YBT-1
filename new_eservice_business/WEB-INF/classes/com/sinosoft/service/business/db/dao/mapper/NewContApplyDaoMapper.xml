		<result column="PROPOSALCONTNO" jdbcType="VARCHAR" property="proposalcontno" />
		<result column="bankcode" jdbcType="VARCHAR" property="bankcode" />
		<result column="polapplydate" jdbcType="VARCHAR" property="polapplydate" />
		<result column="Operator" jdbcType="VARCHAR" property="operator" />
		<result column="appntname" jdbcType="VARCHAR" property="appntname" />
		<result column="cardflag" jdbcType="VARCHAR" property="cardflag" />
		<result column="insurancecom" jdbcType="VARCHAR" property="insurancecom" />
		<result column="transno" jdbcType="VARCHAR" property="transno" />
	</resultMap>
	
	<update id="cancelInsure">
	 	update lccont set appflag =#{appflag} where CONTNO = #{contNo}
	</update>
	
	
	
	<!-- 投保单查询 -->
	<select id="querylccont" resultMap="ResultMap">
		select PROPOSALCONTNO,
               polapplydate,
               Operator,
               appntname,
               transno,
               grpcontno,
                decode(a.appflag,
                      '01',
                      '未试算',
                      '02',
                      '试算成功 ',
                      '03',
                      '扣款成功',
                      '04',
                      '犹豫期申请中',
                      '05',
                      '犹豫期退保成功',
                      '06',
                      '撤单成功',
                      '07',
                      '取消投保',
                      '08',
                      '保险公司返回为非实时保单',
                      '09',
                      '非实时申请',
                      '10',
                      '试算失败',
                      '未知') as appflag,
               insurancecom,
               decode(EXECUTECOM,
                      'CCICHM101',
                      '财产险',
                      'TOPUP',
                      '投连追加',
                      '传统寿险') as executecom
          from lccont a
          
          
          <where>
          		a.forceUWReason is null
          		<if test="contNo != null and contNo != ''">
          			and PROPOSALCONTNO like concat(#{contNo},'%')
        	    </if>
          		
          		 <if test="contNo == '' and operator != ''" >
          			and operator = #{operator}
         		 </if>
         		 
				<if test="grpcontNo != null and grpcontNo != ''">
					and GRPCONTNO = #{grpcontNo}
				</if>
				<!--  
				<if test="transactionNo != null and transactionNo != ''">
					and AGENTBANKCODE = #{transactionNo}
				</if> 
				-->
				<if test="tBRBirth != null" >
					and PolApplyDate = #{tBRBirth,jdbcType=DATE}
				</if>
         		 
          </where>
          	order by transno desc
           <!-- 	-->
	</select>
	
	
