			<select id="contImageSelect" resultType="com.sinosoft.service.business.db.vo.ContImageManageVo">
				select 
				a.proposalcontno as proposalcontno,
				a.appntname as appntname,
				(select name from lacom where agentcom=a.insurancecom) as insuranceName,
				a.insurancecom as agentCode,
				c.comname as comName, 
				c.comcode as comcode,
				e.emplid as emplid,
				
				(select modifiedname from lduploaddoc where prtno = a.proposalcontno ) as modifiedName,
				(select docname from lduploaddoc where prtno = a.proposalcontno ) as docName,
				(select path from lduploaddoc where prtno = a.proposalcontno ) as path
				from lccont a,ldcom c,lduser e 
				where  c.comcode = a.managecom and e.emplid = a.agentcode  
				and e.emplID =#{operator}
				
				<if test="contNo !=null and contNo != ''">
					and a.proposalcontno = #{contNo}
				</if>
				<if test="tBRBirth != null" >
					and a.PolApplyDate = #{tBRBirth,jdbcType=DATE}
				</if>
			</select>
			
